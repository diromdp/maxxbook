{
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx",
    "src"
, "src/app/(cms)/admin/user/page.js", "src/app/(cms)/admin/category/page.js", "src/app/(pages)/[locale]/layout.jsx", "src/app/(cms)/admin/bank/add/page.jsx", "src/app/(cms)/admin/bank/[slug]/page.jsx", "src/app/(cms)/admin/user/add/page.jsx", "src/app/(cms)/admin/category/add/page.jsx", "src/app/(cms)/admin/user/[slug]/page.jsx", "src/app/(cms)/admin/subcategory/add/page.jsx", "src/app/(cms)/admin/subcategory/[slug]/page.jsx", "src/app/(cms)/admin/document/add/page.jsx", "src/app/(cms)/admin/document/[slug]/page.jsx", "src/app/(cms)/admin/category/[slug]/page.jsx" ],
  "exclude": [
    "node_modules"
  ]
}
